    "def max_score(nums, n):\n",
    "    nums.sort(reverse=True)\n",
    "\n",
    "    avg = sum(nums) / n\n",
    "    large_2 = nums[0]\n",
    "    if(n>1):\n",
    "        large_2 = nums[1]\n",
    "\n",
    "    return max(avg, large_2)\n",
    "\n",
    "\n",
    "n = int(input())\n",
    "nums = list(map(int,input().split()))\n",
    "\n",
    "print(max_score(nums, n))\n"
   ]
  }
