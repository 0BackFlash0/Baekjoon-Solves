    "def salt_delivery(weight, big, small):\n",
    "    rest = weight%big\n",
    "\n",
    "    while rest%small!=0:\n",
    "        rest += big\n",
    "        \n",
    "    if(rest>weight):\n",
    "        return -1\n",
    "    return (weight-rest)//5 + rest//3\n",
    "\n",
    "\n",
    "\n",
    "num = int(input())\n",
    "print(salt_delivery(num, 5, 3))"
   ]
